source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/shvets/Specs.git'

use_frameworks!

target 'KinoTochkaSite_iOS' do
  platform :ios, '10.0'

  podspec :path => 'KinoTochkaSite.podspec'

  target 'KinoTochkaSite_iOSTests' do
    inherit! :search_paths
  end
end

target 'KinoTochkaSite_tvOS' do
  platform :tvos, '10.10'

  podspec :path => 'KinoTochkaSite.podspec'

  target 'KinoTochkaSite_tvOSTests' do
    inherit! :search_paths
  end
end

